
public class Players {

	protected String name;
	protected String game;
	
	public Players(String name) {
		
	}
	
	public void setName(String name) {
		this.name = name;
	}
	
	public String getName() {
		return name;
	}
	
	public void setGame(String game) {
		this.game = game;
	}
	
	public String getGame() {
		return game;
	}
	
}
